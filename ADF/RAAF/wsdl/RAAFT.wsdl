<?xml version= '1.0' encoding= 'UTF-8' ?>
<wsdl:definitions
     name="RAAFT"
     targetNamespace="http://xmlns.oracle.com/ADF_C4ISR/RAAF/RAAFT"
     xmlns:tns="http://xmlns.oracle.com/ADF_C4ISR/RAAF/RAAFT"
     xmlns:inp1="http://example.com/fighterSchema"
     xmlns:inp2="http://example.com/weaponSchema"
     xmlns:inp3="http://example.com/payloadSchema"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema"
     xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/"
     xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/"
    >
    <wsdl:types>
        <xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema">
            <xsd:import namespace="http://example.com/fighterSchema" schemaLocation="../schema/Fighters.xsd"/>
        </xsd:schema>
        <xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema">
            <xsd:import namespace="http://example.com/weaponSchema" schemaLocation="../schema/Weapons.xsd"/>
        </xsd:schema>
        <xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema">
            <xsd:import namespace="http://example.com/payloadSchema" schemaLocation="../schema/Payload.xsd"/>
        </xsd:schema>
    </wsdl:types>
    <wsdl:message name="requestMessage">
        <wsdl:part name="fighterRequest" element="inp1:getFighterRequest"/>
        <wsdl:part name="weaponRequest" element="inp2:getWeaponRequest"/>
    </wsdl:message>
    <wsdl:message name="replyMessage">
        <wsdl:part name="payloadResponse" element="inp3:getPayloadResponse"/>
    </wsdl:message>
    <wsdl:portType name="RAAFT_ptt">
        <wsdl:operation name="getPayload">
            <wsdl:input message="tns:requestMessage"/>
            <wsdl:output message="tns:replyMessage"/>
        </wsdl:operation>
    </wsdl:portType>
    <wsdl:binding name="RAAFT_bind" type="tns:RAAFT_ptt">
        <soap:binding transport="http://schemas.xmlsoap.org/soap/http"/>
        <wsdl:operation name="getPayload">
            <soap:operation style="document" soapAction="getPayload"/>
            <wsdl:input>
                <soap:body use="literal" namespace="http://xmlns.oracle.com/ADF_C4ISR/RAAF/RAAFT" parts="weaponRequest"/>
                <soap:header message="tns:requestMessage" part="fighterRequest" use="literal"/>
            </wsdl:input>
            <wsdl:output>
                <soap:body use="literal" namespace="http://xmlns.oracle.com/ADF_C4ISR/RAAF/RAAFT"/>
            </wsdl:output>
        </wsdl:operation>
    </wsdl:binding>
</wsdl:definitions>
