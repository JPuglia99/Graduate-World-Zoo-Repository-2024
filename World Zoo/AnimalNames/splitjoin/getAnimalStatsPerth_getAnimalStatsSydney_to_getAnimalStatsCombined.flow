<?xml version="1.0" encoding="UTF-8"?>
<bpel:process name="getAnimalStatsPerth_getAnimalStatsSydney_to_getAnimalStatsCombined"
              targetNamespace="http://xmlns.oracle.com/World_Zoo/AnimalNames/"
              xmlns:tns="http://xmlns.oracle.com/World_Zoo/AnimalNames/"
              xmlns:bpel="http://docs.oasis-open.org/wsbpel/2.0/process/executable"
              xmlns:rescon="http://www.bea.com/alsb/flow/resources/config"
              xmlns:bea="http://www.bea.com/bpel/ui/extensions" xmlns:ext="http://www.bea.com/bpel/extensions"
              xmlns:expr="http://www.bea.com/wli/sb/stages/config"
              xmlns:trf="http://www.bea.com/wli/sb/stages/transform/config"
              xmlns:bind="http://example.com/worldzooanimals"
              bea:name="getAnimalStatsPerth_getAnimalStatsSydney_to_getAnimalStatsCombined" rescon:snippetVersion="1.0"
              xmlns:ns0="http://example.com/animalSchema" xmlns:ns1="http://www.bea.com/wli/sb/reference"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xmlns:ns2="http://www.bea.com/wli/sb/stages/logging/config">
  <bpel:partnerLinks>
    <bpel:partnerLink name="getAnimalStatsPerth_getAnimalStatsSydney_to_getAnimalStatsCombined"
                      partnerLinkType="tns:getAnimalStatsPerth_getAnimalStatsSydney_to_getAnimalStatsCombined"
                      myRole="getAnimalStatsPerth_getAnimalStatsSydney_to_getAnimalStatsCombined"></bpel:partnerLink>
    <bpel:partnerLink name="PerthZooBusinessService" partnerLinkType="tns:PerthZooBusinessService"
                      partnerRole="PerthZooBusinessService"/>
    <bpel:partnerLink name="SydneyZooBusinessService" partnerLinkType="tns:SydneyZooBusinessService"
                      partnerRole="SydneyZooBusinessService"/>
  </bpel:partnerLinks>
  <bpel:variables>
    <bpel:variable name="request" messageType="bind:getAnimalStatsRequestMessage"></bpel:variable>
    <bpel:variable name="getAnimalStatsSydney" messageType="bind:getAnimalStatsResponseMessage"></bpel:variable>
    <bpel:variable name="getAnimalStatsPerth" messageType="bind:getAnimalStatsResponseMessage"/>
    <bpel:variable name="getAnimalStatsCombined" messageType="bind:getAnimalStatsResponseMessage"/>
  </bpel:variables>
  <bpel:sequence>
    <bpel:receive partnerLink="getAnimalStatsPerth_getAnimalStatsSydney_to_getAnimalStatsCombined"
                  operation="getAnimalStats" createInstance="yes" variable="request">
      <rescon:receiveInfo>
        <rescon:wsdl ref="AnimalNames/wsdl/WorldZooAnimals" binding="bind:WorldZooAnimalsBinding"/>
      </rescon:receiveInfo>
    </bpel:receive>
    <bpel:assign>
      <bpel:extensionAssignOperation>
        <ns2:log>
          <ns2:expr>
            <expr:xqueryText>$request.requestPart</expr:xqueryText>
          </ns2:expr>
          <ns2:logLevel>error</ns2:logLevel>
          <ns2:message>~~~~~~request split join receive logging~~~~~</ns2:message>
        </ns2:log>
      </bpel:extensionAssignOperation>
    </bpel:assign>
    <bpel:assign>
      <bpel:extensionAssignOperation>
        <trf:assign varName="getAnimalStatsCombined.responsePart">
          <trf:expr>
            <expr:xqueryText>&lt;getAnimalStatsResponse&gt;&lt;/getAnimalStatsResponse&gt;</expr:xqueryText>
          </trf:expr>
        </trf:assign>
      </bpel:extensionAssignOperation>
    </bpel:assign>
    <bpel:flow>
      <bpel:scope bea:name="Perth Scope">
        <bpel:sequence>
          <bpel:invoke partnerLink="PerthZooBusinessService" operation="getAnimalStats" inputVariable="request"
                       outputVariable="getAnimalStatsPerth">
            <rescon:invokeInfo>
              <rescon:service ref="AnimalNames/business/PerthZooBusinessService" xsi:type="ns1:BusinessServiceRef"/>
            </rescon:invokeInfo>
          </bpel:invoke>
          <bpel:assign>
            <bpel:extensionAssignOperation>
              <ns2:log>
                <ns2:expr>
                  <expr:xqueryText>$getAnimalStatsPerth.responsePart</expr:xqueryText>
                </ns2:expr>
                <ns2:logLevel>error</ns2:logLevel>
                <ns2:message>~~~~~~~~~~Perth ~~~~~~~~~~</ns2:message>
              </ns2:log>
            </bpel:extensionAssignOperation>
          </bpel:assign>
        </bpel:sequence>
      </bpel:scope>
      <bpel:scope bea:name="Sydney Scope">
        <bpel:sequence>
          <bpel:invoke partnerLink="SydneyZooBusinessService" operation="getAnimalStats" inputVariable="request"
                       outputVariable="getAnimalStatsSydney">
            <rescon:invokeInfo>
              <rescon:service ref="AnimalNames/business/SydneyZooBusinessService" xsi:type="ns1:BusinessServiceRef"/>
            </rescon:invokeInfo>
          </bpel:invoke>
          <bpel:assign>
            <bpel:extensionAssignOperation>
              <ns2:log>
                <ns2:expr>
                  <expr:xqueryText>$getAnimalStatsSydney.responsePart</expr:xqueryText>
                </ns2:expr>
                <ns2:message>~~~~~~~~~~Sydney ~~~~~~~~~~</ns2:message>
                <ns2:logLevel>error</ns2:logLevel>
              </ns2:log>
            </bpel:extensionAssignOperation>
          </bpel:assign>
        </bpel:sequence>
      </bpel:scope>
    </bpel:flow>
    <bpel:assign>
      <bpel:extensionAssignOperation>
        <trf:replace contents-only="false" varName="getAnimalStatsCombined.responsePart">
          <trf:expr>
            <expr:xqueryTransform>
              <expr:resource ref="AnimalNames/xquery/getAnimalStatsPerth_getAnimalStatsSydney_to_CombinedAnimalStats"/>
              <expr:param name="getPerthAnimals">
                <expr:path>$getAnimalStatsPerth.responsePart</expr:path>
              </expr:param>
              <expr:param name="getSydAnimals">
                <expr:path>$getAnimalStatsSydney.responsePart</expr:path>
              </expr:param>
            </expr:xqueryTransform>
          </trf:expr>
        </trf:replace>
      </bpel:extensionAssignOperation>
      <bpel:extensionAssignOperation>
        <ns2:log>
          <ns2:expr>
            <expr:xqueryText>$getAnimalStatsCombined.responsePart</expr:xqueryText>
          </ns2:expr>
          <ns2:logLevel>error</ns2:logLevel>
          <ns2:message>~~~~~~request split join replace logging~~~~~</ns2:message>
        </ns2:log>
      </bpel:extensionAssignOperation>
    </bpel:assign>
    <bpel:reply partnerLink="getAnimalStatsPerth_getAnimalStatsSydney_to_getAnimalStatsCombined"
                operation="getAnimalStats" variable="getAnimalStatsCombined"></bpel:reply>
  </bpel:sequence>
</bpel:process>